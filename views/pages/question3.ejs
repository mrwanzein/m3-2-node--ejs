<%- include('../partials/questions/HTMLheader') %>
<body>
  <header>
    <h1>Node: Workshop 2</h1>
  </header>
  <div class="main">
    <h2>Part 1</h2>
    <div class="content">
      <%- include('../partials/questions/3') %>
      <div class="box answer">
        <h3>Answer</h3>
        <ul>
          <% for(const prop in homer) { %> 
            <% if (prop === 'imageUrl') { %>
              <%- `<img src=${homer[prop]} alt="Homer Simpson">` %>
            <% } else { %>
              <%- `<li>${prop}: ${homer[prop]}</li>` %>
              <% } %>
          <% }; %> 
        </ul>
      </div>
    </div>
  </div>
</body>
